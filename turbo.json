{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"],
      "env": ["PORT"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
